{
    "version": 2,
    "builds": [
        {
            "src": "api/.ts",
            "use": "@vercel/node"
        },
        {
            "src": "web/",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "web/index.html"
        }
    ]
}